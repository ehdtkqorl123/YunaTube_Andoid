<?xml version="1.0" encoding="utf-8"?>
<ca.paulshin.yunatube.widgets.SquareLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <View
            android:id="@+id/view"
            android:layout_width="@dimen/main_layout_inner_padding"
            android:layout_height="10dp"
            android:layout_centerHorizontal="true"/>

        <FrameLayout
            android:id="@+id/insta_frame_0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toLeftOf="@id/view"
            android:layout_toStartOf="@id/view"
            android:clickable="true"
            android:foreground="?selectableItemBackground">

            <ca.paulshin.yunatube.widgets.SquareBezelImageView
                android:id="@+id/insta_thumb_0"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scaleType="centerCrop"
                android:src="@drawable/placeholder_gray"
                app:maskDrawable="@drawable/bezel_background_mask"/>

            <ImageView
                android:id="@+id/insta_video_play_0"
                android:layout_width="@dimen/main_instagram_video_icon_dimen"
                android:layout_height="@dimen/main_instagram_video_icon_dimen"
                android:layout_gravity="top|end"
                android:src="@drawable/instagram_play"/>
        </FrameLayout>

        <LinearLayout
            android:id="@+id/temp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/view"
            android:layout_toRightOf="@id/view"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/main_layout_inner_padding"
                android:layout_weight="1"
                android:orientation="vertical">

                <FrameLayout
                    android:id="@+id/insta_frame_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/main_layout_inner_padding"
                    android:layout_weight="1"
                    android:clickable="true"
                    android:foreground="?selectableItemBackground">

                    <ca.paulshin.yunatube.widgets.SquareBezelImageView
                        android:id="@+id/insta_thumb_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/placeholder_gray"
                        app:maskDrawable="@drawable/bezel_background_mask"/>

                    <ImageView
                        android:id="@+id/insta_video_play_1"
                        android:layout_width="@dimen/main_instagram_video_icon_dimen"
                        android:layout_height="@dimen/main_instagram_video_icon_dimen"
                        android:layout_gravity="top|end"
                        android:src="@drawable/instagram_play"/>
                </FrameLayout>


                <FrameLayout
                    android:id="@+id/insta_frame_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:foreground="?selectableItemBackground">

                    <ca.paulshin.yunatube.widgets.SquareBezelImageView
                        android:id="@+id/insta_thumb_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/placeholder_gray"
                        app:maskDrawable="@drawable/bezel_background_mask"/>

                    <ImageView
                        android:id="@+id/insta_video_play_2"
                        android:layout_width="@dimen/main_instagram_video_icon_dimen"
                        android:layout_height="@dimen/main_instagram_video_icon_dimen"
                        android:layout_gravity="top|end"
                        android:src="@drawable/instagram_play"/>
                </FrameLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <FrameLayout
                    android:id="@+id/insta_frame_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/main_layout_inner_padding"
                    android:clickable="true"
                    android:foreground="?selectableItemBackground">

                    <ca.paulshin.yunatube.widgets.SquareBezelImageView
                        android:id="@+id/insta_thumb_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/placeholder_gray"
                        app:maskDrawable="@drawable/bezel_background_mask"/>

                    <ImageView
                        android:id="@+id/insta_video_play_3"
                        android:layout_width="@dimen/main_instagram_video_icon_dimen"
                        android:layout_height="@dimen/main_instagram_video_icon_dimen"
                        android:layout_gravity="top|end"
                        android:src="@drawable/instagram_play"/>
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/insta_frame_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:clickable="true"
                    android:foreground="?selectableItemBackground">

                    <ca.paulshin.yunatube.widgets.SquareBezelImageView
                        android:id="@+id/insta_thumb_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/placeholder_gray"
                        app:maskDrawable="@drawable/bezel_background_mask"/>

                    <ImageView
                        android:id="@+id/insta_video_play_4"
                        android:layout_width="@dimen/main_instagram_video_icon_dimen"
                        android:layout_height="@dimen/main_instagram_video_icon_dimen"
                        android:layout_gravity="top|end"
                        android:src="@drawable/instagram_play"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@id/insta_frame_0"
            android:layout_marginTop="@dimen/main_layout_inner_padding"
            android:layout_toLeftOf="@id/view"
            android:layout_toStartOf="@id/view"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/main_layout_inner_padding"
                android:layout_weight="1"
                android:orientation="vertical">

                <FrameLayout
                    android:id="@+id/insta_frame_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/main_layout_inner_padding"
                    android:layout_weight="1"
                    android:clickable="true"
                    android:foreground="?selectableItemBackground">

                    <ca.paulshin.yunatube.widgets.SquareBezelImageView
                        android:id="@+id/insta_thumb_5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/placeholder_gray"
                        app:maskDrawable="@drawable/bezel_background_mask"/>

                    <ImageView
                        android:id="@+id/insta_video_play_5"
                        android:layout_width="@dimen/main_instagram_video_icon_dimen"
                        android:layout_height="@dimen/main_instagram_video_icon_dimen"
                        android:layout_gravity="top|end"
                        android:src="@drawable/instagram_play"/>
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/insta_frame_6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:clickable="true"
                    android:foreground="?selectableItemBackground">

                    <ca.paulshin.yunatube.widgets.SquareBezelImageView
                        android:id="@+id/insta_thumb_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/placeholder_gray"
                        app:maskDrawable="@drawable/bezel_background_mask"/>

                    <ImageView
                        android:id="@+id/insta_video_play_6"
                        android:layout_width="@dimen/main_instagram_video_icon_dimen"
                        android:layout_height="@dimen/main_instagram_video_icon_dimen"
                        android:layout_gravity="top|end"
                        android:src="@drawable/instagram_play"/>
                </FrameLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <FrameLayout
                    android:id="@+id/insta_frame_7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/main_layout_inner_padding"
                    android:layout_weight="1"
                    android:clickable="true"
                    android:foreground="?selectableItemBackground">

                    <ca.paulshin.yunatube.widgets.SquareBezelImageView
                        android:id="@+id/insta_thumb_7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/placeholder_gray"
                        app:maskDrawable="@drawable/bezel_background_mask"/>

                    <ImageView
                        android:id="@+id/insta_video_play_7"
                        android:layout_width="@dimen/main_instagram_video_icon_dimen"
                        android:layout_height="@dimen/main_instagram_video_icon_dimen"
                        android:layout_gravity="top|end"
                        android:src="@drawable/instagram_play"/>
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/insta_frame_8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:clickable="true"
                    android:foreground="?selectableItemBackground">

                    <ca.paulshin.yunatube.widgets.SquareBezelImageView
                        android:id="@+id/insta_thumb_8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/placeholder_gray"
                        app:maskDrawable="@drawable/bezel_background_mask"/>

                    <ImageView
                        android:id="@+id/insta_video_play_8"
                        android:layout_width="@dimen/main_instagram_video_icon_dimen"
                        android:layout_height="@dimen/main_instagram_video_icon_dimen"
                        android:layout_gravity="top|end"
                        android:src="@drawable/instagram_play"/>
                </FrameLayout>

            </LinearLayout>
        </LinearLayout>

        <FrameLayout
            android:id="@+id/insta_frame_9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/temp"
            android:layout_marginTop="@dimen/main_layout_inner_padding"
            android:layout_toEndOf="@id/view"
            android:layout_toRightOf="@id/view"
            android:clickable="true"
            android:foreground="?selectableItemBackground">

            <ca.paulshin.yunatube.widgets.SquareBezelImageView
                android:id="@+id/insta_thumb_9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scaleType="centerCrop"
                android:src="@drawable/placeholder_gray"
                app:maskDrawable="@drawable/bezel_background_mask"/>

            <ImageView
                android:id="@+id/insta_video_play_9"
                android:layout_width="@dimen/main_instagram_video_icon_dimen"
                android:layout_height="@dimen/main_instagram_video_icon_dimen"
                android:layout_gravity="top|end"
                android:src="@drawable/instagram_play"/>
        </FrameLayout>
    </RelativeLayout>
</ca.paulshin.yunatube.widgets.SquareLinearLayout>
